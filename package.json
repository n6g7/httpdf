{
  "name": "httpdf",
  "version": "0.0.10",
  "description": "PDF generation over HTTP",
  "main": "src/index.js",
  "author": "Nathan Gaberel <nathan@gnab.fr>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:app": "babel src --out-dir build",
    "build:doc": "babel --config-file ./.babelrc",
    "dev": "yarn build:app && PORT=8000 HTTPDF_DOCUMENTS_SRC=./__tests__/documents HTTPDF_DOCUMENTS_DIST=.build DEBUG=httpdf:* yarn start",
    "prebuild": "node build/prebuild.js",
    "pretest": "yarn test:format && yarn test:lint",
    "start": "node build/index.js",
    "test": "yarn test:unit",
    "test:format": "prettier -l \"src/**/*.js\"",
    "test:lint": "eslint src",
    "test:unit": "HTTPDF_DOCUMENTS_SRC=./__tests__/documents HTTPDF_DOCUMENTS_DIST=.build yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@react-pdf/renderer": "^3.0.3",
    "body-parser": "^1.20.1",
    "chokidar": "^3.5.3",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "axios": "^1.2.3",
    "axiosist": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.3"
  }
}
