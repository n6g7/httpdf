{
  "name": "httpdf",
  "version": "0.0.4",
  "description": "PDF generation over HTTP",
  "main": "src/app.js",
  "author": "Nathan Gaberel <nathan@gnab.fr>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir build",
    "build:docs": "babel $HTTPDF_DOCUMENTS_SRC --out-dir $HTTPDF_DOCUMENTS_DIST --config-file ./.babelrc",
    "dev": "yarn build && HTTPDF_DOCUMENTS_DIST=./__tests__/documents DEBUG=httpdf:* yarn start",
    "pretest": "yarn test:format && yarn test:lint",
    "start": "node build/app.js",
    "test": "yarn test:unit",
    "test:format": "prettier -l \"src/**/*.js\"",
    "test:lint": "eslint src",
    "test:unit": "jest"
  },
  "dependencies": {
    "@react-pdf/node": "^0.7.6",
    "@react-pdf/renderer": "^1.0.0-alpha.18",
    "debug": "^4.0.1",
    "decache": "^4.4.1",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "walk": "^2.3.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.2"
  }
}
