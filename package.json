{
  "name": "httpdf",
  "version": "0.0.8",
  "description": "PDF generation over HTTP",
  "main": "src/app.js",
  "author": "Nathan Gaberel <nathan@gnab.fr>",
  "license": "MIT",
  "scripts": {
    "build:app": "babel src --out-dir build",
    "build:doc": "babel --config-file ./.babelrc",
    "dev": "yarn build:app && PORT=8000 HTTPDF_DOCUMENTS_SRC=./__tests__/documents HTTPDF_DOCUMENTS_DIST=.build DEBUG=httpdf:* yarn start",
    "prebuild": "node build/prebuild.js",
    "pretest": "yarn test:format && yarn test:lint",
    "start": "node build/app.js",
    "test": "yarn test:unit",
    "test:run": "(yarn start &) && (yarn test)",
    "test:format": "prettier -l \"src/**/*.js\"",
    "test:lint": "eslint src",
    "test:unit": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@react-pdf/renderer": "^1.6.3",
    "chokidar": "^3.0.1",
    "debug": "^4.1.1",
    "decache": "^4.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2"
  }
}
